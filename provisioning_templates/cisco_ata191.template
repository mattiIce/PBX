<!-- ============================================================================ -->
<!-- System Parameters -->
<!-- ============================================================================ -->

<!-- Network Configuration -->
<Internet_Connection_Type>DHCP</Internet_Connection_Type>
<Static_IP></Static_IP>
<NetMask></NetMask>
<Gateway></Gateway>
<Primary_DNS></Primary_DNS>
<Secondary_DNS></Secondary_DNS>
<Host_Name>ATA191</Host_Name>
<Domain></Domain>

<!-- VLAN Configuration -->
<Enable_VLAN>No</Enable_VLAN>
<VLAN_ID></VLAN_ID>

<!-- Power over Ethernet (PoE) - ATA 191 specific -->
<PoE_Enable>Yes</PoE_Enable>

<!-- SIP Parameters -->
<SIP_Port>{{SIP_PORT}}</SIP_Port>
<RTP_Port_Min>16384</RTP_Port_Min>
<RTP_Port_Max>16482</RTP_Port_Max>
<Max_Forward>70</Max_Forward>
<Max_Redirection>5</Max_Redirection>
<Max_Auth>2</Max_Auth>

<!-- QoS/ToS Settings -->
<SIP_TOS_DiffServ_Value>0x68</SIP_TOS_DiffServ_Value>
<RTP_TOS_DiffServ_Value>0xb8</RTP_TOS_DiffServ_Value>
<Enable_QOS>Yes</Enable_QOS>

<!-- SIP Transport -->
<SIP_Transport_1_>UDP</SIP_Transport_1_>
<!-- Options: UDP, TCP, TLS -->

<!-- SIP Timer Values (in seconds) -->
<SIP_T1>0.5</SIP_T1>
<SIP_T2>4</SIP_T2>
<SIP_T4>5</SIP_T4>
<SIP_Timer_B>32</SIP_Timer_B>
<SIP_Timer_F>32</SIP_Timer_F>
<SIP_Timer_H>32</SIP_Timer_H>
<SIP_Timer_D>32</SIP_Timer_D>
<SIP_Timer_J>32</SIP_Timer_J>

<!-- SIP Session Timer -->
<Session_Expires>180</Session_Expires>
<Min_SE>90</Min_SE>
<Caller_Request_Timer>No</Caller_Request_Timer>
<Callee_Request_Timer>No</Callee_Request_Timer>
<Force_Timer>No</Force_Timer>
<UAC_Session_Expires>No</UAC_Session_Expires>
<UAS_Session_Expires>No</UAS_Session_Expires>

<!-- SIP Behavior -->
<Use_Compact_Header>No</Use_Compact_Header>
<Remove_Last_Reg>No</Remove_Last_Reg>

<!-- Regional Settings (FCC/North America) -->
<Ring_Waveform>Sinusoid</Ring_Waveform>
<Ring_Frequency>20</Ring_Frequency>
<Ring_Voltage>75</Ring_Voltage>
<FXS_Port_Impedance>600</FXS_Port_Impedance>
<!-- Options: 600 (North America), 270, 370, 600+2.16, 900, 220+820, 220+820+120 -->

<!-- Hook Flash Timer -->
<Hook_Flash_Timer_Min>0.1</Hook_Flash_Timer_Min>
<Hook_Flash_Timer_Max>0.9</Hook_Flash_Timer_Max>

<!-- Dial Plan -->
<Dial_Plan_1_>(xxxxxxxxxxxx|*x|*xx|*xxx|*xxxx|xxxxxxxxxx|xxxxxxxxxxx|1xxxxxxxxxx|1xxxxxxxxxxx|011xx.)</Dial_Plan_1_>
<Interdigit_Long_Timer>10</Interdigit_Long_Timer>
<Interdigit_Short_Timer>3</Interdigit_Short_Timer>

<!-- FXS Port Timers -->
<Off_Hook_Speed>Fast</Off_Hook_Speed>
<On_Hook_Speed>Normal</On_Hook_Speed>

<!-- Security -->
<!-- Note: TLS/SRTP support depends on PBX capabilities -->
<!-- Set to No if PBX does not support these features -->
<SIP_TLS_Enable>No</SIP_TLS_Enable>
<SRTP_Enable>No</SRTP_Enable>
<!-- SRTP Options: Disabled, Enabled but not forced, Enabled and forced -->
<SRTP_Crypto_Suite_List>AES_CM_128_HMAC_SHA1_80,AES_CM_128_HMAC_SHA1_32</SRTP_Crypto_Suite_List>

<!-- Web Server -->
<Enable_Web_Server>Yes</Enable_Web_Server>
<Web_Server_Port>80</Web_Server_Port>
<Enable_Web_Admin_Access>Yes</Enable_Web_Admin_Access>

<!-- Syslog -->
<Syslog_Server></Syslog_Server>
<Debug_Server></Debug_Server>
<Debug_Level>0</Debug_Level>
<SIP_Debug_Option>0</SIP_Debug_Option>

<!-- Miscellaneous -->
<Primary_DNS></Primary_DNS>
<Secondary_DNS></Secondary_DNS>
<Language_Selection>English</Language_Selection>
<Dictionary_Server_Script></Dictionary_Server_Script>

<!-- Supplementary Services -->
<Supplementary_Service_Subscription>package=full</Supplementary_Service_Subscription>

</flat-profile>
